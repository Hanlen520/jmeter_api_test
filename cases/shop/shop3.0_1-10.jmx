public static connectMySql(String query){
	//数据库连接字段
	String drive = vars.get(&quot;db_driver&quot;);
	String url = vars.get(&quot;db_url&quot;);
	String dbName = vars.get(&quot;db_name&quot;);
	String user = vars.get(&quot;db_username&quot;);
	String psw = vars.get(&quot;db_password&quot;);
	
	Connection mycon = null;
	Statement mystmt = null;
	ResultSet myrset = null;
	log.info(&quot;-----beanShell断言-------&quot;+drive+&quot;--&quot;+url+&quot;--&quot;+dbName+&quot;--&quot;+user+&quot;--&quot;+psw);
	Class.forName(drive); 
	//try中获取数据库连接
	try{
	 mycon = DriverManager.getConnection(url+dbName, user, psw);
	} catch(SQLException e){
	  	log.error(&quot;数据库连接出错&quot;);
	 }
	 
	 queryData(mycon,query);
}

public static queryData(Connection mycon,String query){
	mystmt = mycon.createStatement();
	myrset = mystmt.executeQuery(query);
	 JsonArray data = new JsonArray();
	 while (myrset.next()){
	  JsonObject jsonObject = new JsonObject();
	  jsonObject.addProperty(&quot;classifyId&quot;,myrset.getString(1));
	  jsonObject.addProperty(&quot;classifyName&quot;,myrset.getString(2));
	  jsonObject.addProperty(&quot;classifyCode&quot;,myrset.getString(4));
	  jsonObject.addProperty(&quot;imageUrl&quot;,myrset.getString(6));
	  // 2级查询
	  String queryPrd = &quot;SELECT count(tag_id) from t_product_classify_tag where classify_code like &apos;&quot;+myrset.getString(4)+&quot;%&apos;;&quot;;
	  mystmtprd = mycon.createStatement();
	  cursor = mystmtprd.executeQuery(queryPrd);
	  while(cursor.next()){
